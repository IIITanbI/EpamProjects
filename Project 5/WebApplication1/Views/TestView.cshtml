@{
    ViewBag.Title = "TestView";
}
@using System.Collections
@using System.Web.Mvc.Html
@using WebApplication1.Models
@model IEnumerable<Object>

@Html.DropDownList("Tables", new List<SelectListItem>()
{
    new SelectListItem()
    {
        Text = "text1",
        Value = "value1"
    },
   new SelectListItem()
    {
        Text = "text2",
        Value = "value2"
    }
}, new
{
    @class = "control-label col-md-2",
    @action = "Create"
});
@{
    if (Model.First() is ClientModel)
    {
        List<ClientModel> list =Model.Cast<ClientModel>().ToList();
        @Html.Partial("ClientView", list);
    }
}


